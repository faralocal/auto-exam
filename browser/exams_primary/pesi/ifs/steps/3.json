[
  {
    "type": "group_excel",
    "file": "./users.xlsx",
    "start_row": 2,
    "actions": [
      {
        "type": "main_frame",
        "sleep": 1
      },
      {
        "title": "Close Exam 1 Modal",
        "type": "refresh",
        "sleep": 2
      },
      {
        "title": "Change Tab",
        "type": "click",
        "tag": "a",
        "attr": "href",
        "value": "#certificate",
        "sleep": 4
      },
      {
        "title": "Start Exam 3",
        "type": "click",
        "tag": "a",
        "text": "CPD Certificate",
        "attr": "data-currentname",
        "value": "CPD Certificate",
        "sleep": 3,
        "array_select_one": 2
      },
      {
        "type": "frame",
        "selector": ".mfp-iframe",
        "sleep": 1
      },
      {
        "title": "## [1] > Evaluation Level",
        "type": "click",
        "attr": "href",
        "value": "#collapseOneQuiz1",
        "tag": "div",
        "ignore": true,
        "sleep": 1
      },
      {
        "type": "click",
        "tag": "a",
        "text": "Resume Quiz",
        "sleep": 5,
        "ignore": true
      },
      {
        "type": "click",
        "tag": "a",
        "class": "NextPageForBeforeQuizMessage",
        "ignore": true,
        "sleep": 3
      },

      {
        "type": "array",
        "tag": "section",
        "class": ".clsQuestion",
        "if_find_text_inside": "What are the two paths to relate to parts?",
        "click": [
          {
            "tag": "label",
            "text": "In-sight and direct access"
          }
        ],
        "sleep": 1
      },
      {
        "type": "array",
        "tag": "section",
        "class": ".clsQuestion",
        "if_find_text_inside": "When in-sight work is not possible, what is recommended?",
        "click": [
          {
            "tag": "label",
            "text": "Direct access"
          }
        ],
        "sleep": 1
      },
      {
        "type": "array",
        "tag": "section",
        "class": ".clsQuestion",
        "if_find_text_inside": "What is the primary purpose of direct access?",
        "click": [
          {
            "tag": "label",
            "text": "Unblending"
          }
        ],
        "sleep": 1
      },
      {
        "type": "array",
        "tag": "section",
        "class": ".clsQuestion",
        "if_find_text_inside": "What part is usually in implicit direct access?",
        "click": [
          {
            "tag": "label",
            "text": "Managers"
          }
        ],
        "sleep": 1
      },
      {
        "type": "array",
        "tag": "section",
        "class": ".clsQuestion",
        "if_find_text_inside": "What are the reasons to use implicit direct access with blended managers?",
        "click": [
          {
            "tag": "label",
            "text": "To help them relax, get to know them, and address fears"
          }
        ],
        "sleep": 1
      },
      {
        "type": "array",
        "tag": "section",
        "class": ".clsQuestion",
        "if_find_text_inside": "As parts listen in to direct access to communication in a session, what opens up?",
        "click": [
          {
            "tag": "label",
            "text": "Self-compassion"
          }
        ],
        "sleep": 1
      },
      {
        "type": "array",
        "tag": "section",
        "class": ".clsQuestion",
        "if_find_text_inside": "What is the first step in the overarching guidelines for attuned direct access with protectors?",
        "click": [
          {
            "tag": "label",
            "text": "Unblend any of your parts as a therapist"
          }
        ],
        "sleep": 1
      },
      {
        "type": "array",
        "tag": "section",
        "class": ".clsQuestion",
        "if_find_text_inside": "If you make a mistake as an IFS therapist, what is the best recommendation?",
        "click": [
          {
            "tag": "label",
            "text": "Apologize readily and acknowledge your parts"
          }
        ],
        "sleep": 1
      },
      {
        "type": "array",
        "tag": "section",
        "class": ".clsQuestion",
        "if_find_text_inside": "What question do you ask the client to connect again with the Self?",
        "click": [
          {
            "tag": "label",
            "text": "Is it okay if I talk to (client’s name) now?"
          }
        ],
        "sleep": 1
      },
      {
        "type": "array",
        "tag": "section",
        "class": ".clsQuestion",
        "if_find_text_inside": "What do we want to avoid as therapists?",
        "click": [
          {
            "tag": "label",
            "text": "Part to part relation"
          }
        ],
        "sleep": 1
      },
      {
        "type": "array",
        "tag": "section",
        "class": ".clsQuestion",
        "if_find_text_inside": "What is the risk of doing direct access of exiles with a therapist?",
        "click": [
          {
            "tag": "label",
            "text": "A part becoming attached to the Self of the therapist"
          }
        ],
        "sleep": 1
      },
      {
        "type": "array",
        "tag": "section",
        "class": ".clsQuestion",
        "if_find_text_inside": "What are the last two steps in explicit direct access with an exile?",
        "click": [
          {
            "tag": "label",
            "text": "Find out what’s happening/ why it took over and invite client to be there too"
          }
        ],
        "sleep": 1
      },
      {
        "type": "array",
        "tag": "section",
        "class": ".clsQuestion",
        "if_find_text_inside": "In the client video example, what did the client find most helpful?",
        "click": [
          {
            "tag": "label",
            "text": "Having the therapist watch and hear what the memory felt like to the client’s younger self"
          }
        ],
        "sleep": 1
      },

      {
        "title": "Save And Submit Exam",
        "type": "click",
        "tag": "button",
        "text": "Save & Submit",
        "sleep": 5
      },
      {
        "title": "## [2] > Evaluation Level",
        "type": "click",
        "tag": "a",
        "attr": "aria-label",
        "value": "Click here For Evaluation",
        "sleep": 5
      },
      {
        "title": "Select",
        "type": "click",
        "tag": "label",
        "attr": "data-single",
        "value": "true",
        "text": "Yes",
        "sleep": 1,
        "array_select_one": 0
      },
      {
        "type": "select",
        "tag": "select",
        "class": "clsDrpKeyRating",
        "option_index": 1,
        "array_select_one": 0
      },
      {
        "type": "select",
        "tag": "select",
        "class": "clsDrpKeyRating",
        "option_index": 1,
        "array_select_one": 1
      },
      {
        "type": "select",
        "tag": "select",
        "class": "clsDrpKeyRating",
        "option_index": 1,
        "array_select_one": 2
      },
      {
        "type": "select",
        "tag": "select",
        "class": "clsDrpKeyRating",
        "option_index": 1,
        "array_select_one": 3
      },
      {
        "type": "select",
        "tag": "select",
        "class": "clsDrpKeyRating",
        "option_index": 1,
        "array_select_one": 4
      },
      {
        "type": "select",
        "tag": "select",
        "class": "clsDrpKeyRating",
        "option_index": 1,
        "sleep": 1,
        "array_select_one": 5
      },
      {
        "type": "select",
        "tag": "select",
        "attr": "data-val",
        "value": "true",
        "option_index": 1,
        "array_select_one": 6
      },
      {
        "type": "select",
        "tag": "select",
        "attr": "data-val",
        "value": "true",
        "option_index": 1,
        "array_select_one": 7
      },
      {
        "type": "click",
        "tag": "input",
        "attr": "aria-label",
        "value": "Thumbs up",
        "array_select_one": 0
      },
      {
        "title": "End Exam 3 section",
        "type": "click",
        "tag": "button",
        "class": "btnSaveChanges",
        "sleep": 7
      }
    ]
  }
]
