[
  {
    "type": "group_excel",
    "file": "./users.xlsx",
    "start_row": 2,
    "actions": [
      {
        "title": "Open URL",
        "type": "goto",
        "url": "https://www.pesi.co.uk/account/login",
        "sleep": 10,
        "max_retries": 4,
        "retry_delay": 10,
        "timeout": 160000
      },
      {
        "title": "Write email",
        "type": "write_excel",
        "tag": "input",
        "attr": "id",
        "value": "Email",
        "write_from_col": 4
      },
      {
        "title": "Write password",
        "type": "write",
        "tag": "input",
        "attr": "id",
        "value": "txtPassword",
        "write": "Asdf@1234",
        "sleep": 2
      },
      {
        "title": "Login",
        "type": "click",
        "tab": "button",
        "attr": "id",
        "value": "btnLogInButton",
        "sleep": 5
      },
      {
        "title": "Write email",
        "type": "write_excel",
        "tag": "input",
        "class": "sqemail",
        "attr": "id",
        "value": "Email",
        "write_from_col": 4,
        "ignore": true
      },
      {
        "title": "Login",
        "tab": "button",
        "text": "Get Free Access",
        "sleep": 5,
        "ignore": true
      },
      {
        "title": "Open URL",
        "type": "goto",
        "url": "https://catalog.pesi.co.uk/guestbook/66769",
        "sleep": 5,
        "max_retries": 4,
        "retry_delay": 10,
        "timeout": 160000
      },
      {
        "title": "Write email",
        "type": "write_excel",
        "tag": "input",
        "attr": "id",
        "value": "Email",
        "ignore": true,
        "write_from_col": 4
      },
      {
        "title": "Login",
        "type": "click",
        "tab": "img",
        "ignore": true,
        "sleep": 5
      },
      {
        "title": "Login",
        "type": "click",
        "tab": "button",
        "attr": "id",
        "value": "btnRegister",
        "ignore": true,
        "sleep": 60
      },
      {
        "title": "Change Tab",
        "type": "click",
        "tag": "a",
        "attr": "href",
        "value": "#courseSchedule",
        "sleep": 4
      },
      {
        "type": "click",
        "tag": "a",
        "text": "CPD Certificate",
        "attr": "data-currentname",
        "value": "CPD Certificate",
        "sleep": 2
      },
      {
        "type": "frame",
        "selector": ".mfp-iframe",
        "sleep": 1
      },
      {
        "type": "click",
        "attr": "href",
        "value": "#collapseOneQuiz1",
        "tag": "div",
        "ignore": true,
        "sleep": 1
      },
      {
        "type": "click",
        "tag": "a",
        "text": "Resume Quiz",
        "sleep": 4,
        "ignore": true
      },
      {
        "type": "click",
        "tag": "a",
        "class": "NextPageForBeforeQuizMessage",
        "ignore": true,
        "sleep": 3
      }
    ]
  }
]
